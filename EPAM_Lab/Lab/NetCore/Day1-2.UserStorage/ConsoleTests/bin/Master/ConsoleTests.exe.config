<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section 
      name="ServiceConfigSection" 
      type="DomainConfig.CustomSections.ServiceConfig.ServiceConfigSection, DomainConfig"/>
  </configSections>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>
  <appSettings>
    <add key="Path" value="./users.xml"/>
  </appSettings>
  <system.diagnostics>
    <switches>
      <add name="Data" value="1" />
    </switches>
  </system.diagnostics>
  <ServiceConfigSection>
    <MasterService type="Master" name="master">
      <add type="Slave" name="slave_1" ip ="127.0.0.1" port="9000" created="true"/>
      <add type="Slave" name="slave_2" ip ="127.0.0.1" port="9001" created="true"/>
      <add type="Slave" name="slave_3" ip ="127.0.0.1" port="9002" created="true"/>
    </MasterService>
  </ServiceConfigSection>
  <system.serviceModel>
    <behaviors>
      <serviceBehaviors>
        <behavior>
          <serviceMetadata httpGetEnabled="True"/>
        </behavior>
      </serviceBehaviors>
    </behaviors>
  </system.serviceModel>
</configuration>